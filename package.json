{
  "name": "hello_world_react",
  "version": "1.0.0",
  "description": "First react js test",
  "author": "GALLOIS Guillaume",
  "scripts": {
    "build-stylus": "stylus index.styl -c -u ./node_modules/autoprefixer-stylus -o app.css",
	"watch-stylus": "stylus -w -m index.styl -u ./node_modules/autoprefixer-stylus -o app.css",
	"watch-js": "watchify index.js -o app.js -v --debug",
	"build-js": "browserify index.js -o app.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "aliasify"
    ]
  },
  "private": true,
  "dependencies": {
    "abyssa": "7.0.1",
    "fluxx": "0.4.0",
    "lodash": "3.9.1",
    "moment": "2.10.6",
    "normalize-css": "2.3.1",
    "numeral": "1.5.3",
    "q": "1.4.0",
    "qajax": "1.3.0",
    "react": "0.14.0",
    "react-dom": "0.14.0"
  },
  "devDependencies": {
    "aliasify": "~1.7.2",
    "autoprefixer-stylus": "~0.7.0",
    "babel-eslint": "~4.1.2",
    "babelify": "~6.1.0",
    "browserify": "~6.3.2",
    "eslint": "~1.4.1",
    "eslint-plugin-react": "~3.3.2",
    "stylus": "~0.51.1",
    "watchify": "~2.1.1"
  }
}
